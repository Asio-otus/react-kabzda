te(currentValue)\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            <select value={state} onChange={onChange}>\r\n                <option>none</option>\r\n                <option value={'1'}>One</option>\r\n                <option value={'2'}>Two</option>\r\n                <option value={'3'}>Three</option>\r\n            </select>\r\n            <div>{state}</div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport const ControlledInputWithFixedValue = () => <input value={'custom text'}/>","D:\\Web\\react-kabzda\\src\\components\\Select\\Select.tsx",[],"D:\\Web\\react-kabzda\\src\\components\\Select\\SelectMyFirstVersion.tsx",["161","162"],"import React, {DetailedHTMLProps, Dispatch, HTMLAttributes, SetStateAction} from \"react\";\r\nimport {selectValueType} from \"../../App\";\r\n\r\ntype itemsType = {\r\n    title: string\r\n    value: any\r\n}\r\n\r\ntype SelectPropsType = {\r\n    selectedValue: selectValueType\r\n    collapsed: boolean\r\n    onChange: (value: any) => void\r\n    items: itemsType[]\r\n    selectValue: Dispatch<SetStateAction<selectValueType>>\r\n}\r\n\r\ntype SelectMenuPropsType = {\r\n    selectedValue: selectValueType\r\n    collapsed: boolean\r\n    onChange: (value: any) => void\r\n    items: itemsType[]\r\n    selectValue: Dispatch<SetStateAction<selectValueType>>\r\n}\r\n\r\nexport function SelectMyFirstVersion(props: SelectPropsType) {\r\n    return (\r\n        <div>\r\n            <h3 onClick={() => {\r\n                props.onChange(!props.collapsed)\r\n            }}>{props.selectedValue.title}</h3>\r\n            {!props.collapsed && <SelectMenu\r\n                selectedValue={props.selectedValue}\r\n                items={props.items}\r\n                selectValue={props.selectValue}\r\n                onChange={props.onChange}\r\n                collapsed={props.collapsed}/>}\r\n        </div>\r\n    )\r\n}\r\n\r\nfunction SelectMenu(props: SelectMenuPropsType) {\r\n\r\n    return (\r\n        <div>\r\n            {props.items.map(i => <div key={props.selectedValue.value} onClick={() => {\r\n                props.selectValue(i);\r\n                props.onChange(!props.collapsed)\r\n            }}>{i.title}</div>)}\r\n        </div>\r\n    )\r\n}","D:\\Web\\react-kabzda\\src\\components\\Select\\Select.stories.tsx",["163","164"],"import React, {useState} from \"react\";\r\nimport {action} from \"@storybook/addon-actions\";\r\nimport {Select} from \"./Select\";\r\n\r\nexport default {\r\n    title: 'Select',\r\n    component: Select\r\n}\r\n\r\nexport const WithoutValue = () => {\r\n    const [value, setValue] = useState(null)\r\n    return (\r\n        <Select\r\n            onChange={setValue}\r\n            value={value}\r\n            items={[\r\n                {value: '1', title: 'Minsk'},\r\n                {value: '2', title: 'Moscow'},\r\n                {value: '3', title: 'Kiev'}\r\n            ]}/>\r\n    )\r\n}\r\n\r\nexport const WithValue = () => {\r\n    const [value, setValue] = useState('2')\r\n    return (\r\n        <Select\r\n            onChange={setValue}\r\n            value={value}\r\n            items={[\r\n                {value: '1', title: 'Minsk'},\r\n                {value: '2', title: 'Moscow'},\r\n                {value: '3', title: 'Kiev'}\r\n            ]}/>\r\n    )\r\n}","D:\\Web\\react-kabzda\\src\\components\\AccordionUncontrolled\\reducer.tsx",[],"D:\\Web\\react-kabzda\\src\\stories\\ReactMemo.stories.tsx",["165"],"import React, {useState} from \"react\";\r\n\r\nexport default {\r\n    title: 'React.memo demo'\r\n}\r\n\r\nconst NewMessageCounter = (props: { count: number }) => {\r\n    return <div>{props.count}</div>\r\n}\r\n\r\nconst Users = React.memo((props: { users: Array<string> }) => {\r\n    console.log('USERS')\r\n    return <div>{props.users.map((u, i) => <div key={i}>{u}</div>)}</div>\r\n})\r\n\r\n\r\nexport const Example1 = () => {\r\n    const [counter, setCounter] = useState(0)\r\n    const [users, setUsers] = useState(['Sonya', '