nputRef}/> - <button onClick={save}>save</button> saved value: {value} </>\r\n}\r\n\r\nexport const ControlledInput = () => {\r\n    const [value, setValue] = useState<string>('')\r\n\r\n    const inputChange = (event: ChangeEvent<HTMLInputElement>) => {\r\n        let currentValue = event.currentTarget.value\r\n        setValue(currentValue)\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            <input value={value} onChange={inputChange}/>\r\n            <div>{value}</div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport const ControlledCheckbox = () => {\r\n    const [state, setState] = useState<boolean>(true)\r\n\r\n    const click = () => {\r\n        setState(!state)\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            <input checked={state} type={'checkbox'} onClick={click} />\r\n            <div>{state}</div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport const ControlledSelect = () => {\r\n    const [state, setState] = useState<string | undefined>(undefined)\r\n\r\n    const onChange = (e: ChangeEvent<HTMLSelectElement>) => {\r\n        let currentValue = e.currentTarget.value\r\n        setState(currentValue)\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            <select value={state} onChange={onChange}>\r\n                <option>none</option>\r\n                <option value={'1'}>One</option>\r\n                <option value={'2'}>Two</option>\r\n                <option value={'3'}>Three</option>\r\n            </select>\r\n            <div>{state}</div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport const ControlledInputWithFixedValue = () => <input value={'custom text'}/>","D:\\Web\\react-kabzda\\src\\components\\Select\\Select.tsx",[],"D:\\Web\\react-kabzda\\src\\components\\Select\\SelectMyFirstVersion.tsx",["160","161"],"D:\\Web\\react-kabzda\\src\\components\\Select\\Select.stories.tsx",["162","163"],"import React, {useState} from \"react\";\r\nimport {action} from \"@storybook/addon-actions\";\r\nimport {Select} from \"./Select\";